<configuration>


    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>^V^ [%p] [%d{yyyy-MM-dd HH:mm:ss.SSS}] [%t] [%c:%L] [%X{traceId}:%X{spanId}:%X{parentId}] [%X{req.xRequestId}] %m%n</pattern>
        </encoder>
    </appender>

    <logger name="org.apache.http" level="TRACE"/>


    <root level="TRACE">
        <appender-ref ref="stdout"/>
    </root>

</configuration>